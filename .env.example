# EdgeDelta AI Team Configuration
# Copy this to .env and fill in your values

# Organization ID (required)
ED_ORG_ID=your-org-id-here

# API Token for api.edgedelta.com (required for models/connectors)
ED_API_TOKEN=your-api-token-here

# JWT for chat.ai/agent.ai (optional - can be obtained via login)
ED_JWT=

# Login credentials (optional - used to obtain JWT if ED_JWT not set)
ED_EMAIL=
ED_PASSWORD=
